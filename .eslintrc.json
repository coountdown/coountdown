{
  "parser": "babel-eslint",
  "extends": ["prettier", "eslint:recommended", "airbnb-base"],
  "plugins": ["prettier"],
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-undef": 0,
    "no-console": 0,
    "semi": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
